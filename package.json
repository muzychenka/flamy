{
    "name": "flamy",
    "version": "1.0.0",
    "description": "Telegram бот для знакомств на Telegraf, Prisma и TypeScript",
    "main": "app.ts",
    "scripts": {
        "build": "npx tsc",
        "start": "npm run build && npm run prod",
        "prod": "NODE_ENV=production node dist/app.js",
        "dev": "NODE_ENV=development nodemon ./app.ts",
        "format": "prettier --write ./**/*.{ts}"
    },
    "keywords": [
        "telegram",
        "telegraf",
        "flamy",
        "dating",
        "prisma"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.15.0",
        "@types/dotenv": "^8.2.0",
        "@types/uuid": "^9.0.2",
        "axios": "^1.4.0",
        "cyrillic-to-translit-js": "^3.2.1",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "telegraf": "^4.12.2",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.5",
        "concurrently": "^8.2.0",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.1.6"
    }
}
